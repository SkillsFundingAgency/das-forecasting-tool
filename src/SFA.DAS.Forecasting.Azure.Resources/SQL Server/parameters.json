{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "serverName": {
            "value": "__sqlServerName__"
        },
        "location": {
            "value": "__sqlServerLocation__"
        },
        "administratorLogin": {
            "value": "__sqlServerAdminLogin__"
        },
        "administratorLoginPassword": {
          "reference": {
            "keyVault": {
              "id": "__keyVaultId__"
            },
            "secretName": "__sqlAdminSecret__"
          }
        }
    }
}