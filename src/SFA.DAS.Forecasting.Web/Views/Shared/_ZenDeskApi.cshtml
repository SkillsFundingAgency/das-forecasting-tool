@using SFA.DAS.Forecasting.Web.Filters;

@{ 
    var zendeskApiData = ViewBag.ZendeskApiData as ZendeskApiFilter.ZendeskApiData ?? new ZendeskApiFilter.ZendeskApiData();
}

<script nws-csp-add-nonce="true" type="text/javascript">
    zE(function () {
        zE.identify({
            name: '@zendeskApiData.Name',
            email: '@zendeskApiData.Email',
            organization: '@zendeskApiData.Organization'
        });
    });
</script>